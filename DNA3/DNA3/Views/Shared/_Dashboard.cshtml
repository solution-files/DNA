<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@Configuration["App:Shortname"] | @Configuration["App:Tagline"]</title>

    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">

    <!-- Font Awesome Icons -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css" rel="stylesheet" />

    <!-- IonIcons -->
    <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">

    <!--====== Syncfusion =======-->
    <link rel="stylesheet" href="https://cdn.syncfusion.com/ej2/29.1.37/fluent.css" />
    <script src="https://cdn.syncfusion.com/ej2/29.1.37/dist/ej2.min.js"></script>

    <!-- Custom Utilities -->
    <script src="~/common/js/utils.js"></script>

    <!-- Page Specific CSS -->
    @RenderSection("CSS", required: false)

    <!-- Theme style -->
    <link rel="stylesheet" href="/dashboard/css/adminlte.min.css">

</head>
<!--
`body` tag options:

  Apply one or more of the following classes to to the body tag
  to get the desired effect

  * sidebar-collapse
  * sidebar-mini
-->
<body class="hold-transition sidebar-mini e-bigger">
    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-white navbar-light">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="/Dashboard" class="nav-link">Home</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="/Dashboard/Logout" class="nav-link">Sign Out</a>
                </li>
            </ul>

            <!-- Right navbar links -->
            <ul class="navbar-nav ml-auto">
                <!-- Navbar Search -->
                <li class="nav-item">
                    <a class="nav-link" data-widget="navbar-search" href="#" role="button">
                        <i class="fas fa-search"></i>
                    </a>
                    <div class="navbar-search-block">
                        <form class="form-inline">
                            <div class="input-group input-group-sm">
                                <input class="form-control form-control-navbar" type="search" placeholder="Search" aria-label="Search" id="Criteria" name="Criteria">
                                <div class="input-group-append">
                                    <button class="btn btn-navbar" type="submit">
                                        <i class="fas fa-search"></i>
                                    </button>
                                    <button class="btn btn-navbar" type="button" data-widget="navbar-search-grr" asp-action="Index" asp-method="post">
                                        <i class="fas fa-times"></i>
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                </li>

                <!-- Messages Dropdown Menu -->
                <li class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#">
                        <i class="far fa-comments"></i>
                        <span class="badge badge-danger navbar-badge">3</span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-lg dropdown-menu-right">
                        <a href="#" class="dropdown-item">
                            <!-- Message Start -->
                            <div class="media">
                                <img src="/dashboard/img/user1-128x128.jpg" alt="User Avatar" class="img-size-50 mr-3 img-circle">
                                <div class="media-body">
                                    <h3 class="dropdown-item-title">
                                        Brad Diesel
                                        <span class="float-right text-sm text-danger"><i class="fas fa-star"></i></span>
                                    </h3>
                                    <p class="text-sm">Call me whenever you can...</p>
                                    <p class="text-sm text-muted"><i class="far fa-clock mr-1"></i> 4 Hours Ago</p>
                                </div>
                            </div>
                            <!-- Message End -->
                        </a>
                        <div class="dropdown-divider"></div>
                        <a href="#" class="dropdown-item">
                            <!-- Message Start -->
                            <div class="media">
                                <img src="/dashboard/img/user8-128x128.jpg" alt="User Avatar" class="img-size-50 img-circle mr-3">
                                <div class="media-body">
                                    <h3 class="dropdown-item-title">
                                        John Pierce
                                        <span class="float-right text-sm text-muted"><i class="fas fa-star"></i></span>
                                    </h3>
                                    <p class="text-sm">I got your message bro</p>
                                    <p class="text-sm text-muted"><i class="far fa-clock mr-1"></i> 4 Hours Ago</p>
                                </div>
                            </div>
                            <!-- Message End -->
                        </a>
                        <div class="dropdown-divider"></div>
                        <a href="#" class="dropdown-item">
                            <!-- Message Start -->
                            <div class="media">
                                <img src="/dashboard/img/user3-128x128.jpg" alt="User Avatar" class="img-size-50 img-circle mr-3">
                                <div class="media-body">
                                    <h3 class="dropdown-item-title">
                                        Nora Silvester
                                        <span class="float-right text-sm text-warning"><i class="fas fa-star"></i></span>
                                    </h3>
                                    <p class="text-sm">The subject goes here</p>
                                    <p class="text-sm text-muted"><i class="far fa-clock mr-1"></i> 4 Hours Ago</p>
                                </div>
                            </div>
                            <!-- Message End -->
                        </a>
                        <div class="dropdown-divider"></div>
                        <a href="#" class="dropdown-item dropdown-footer">See All Messages</a>
                    </div>
                </li>
                <!-- Notifications Dropdown Menu -->
                <li class="nav-item dropdown">
                    <a class="nav-link" data-toggle="dropdown" href="#" onclick="GetPartialView('/dashboard/notifications', '#notification-menu')">
                        <i class="far fa-bell"></i>
                        <span class="badge badge-warning navbar-badge">15</span>
                    </a>
                    <div class="dropdown-menu dropdown-menu-xl dropdown-menu-right" id="notification-menu"></div>
                </li>
                <li class="nav-item">
                    <a class="nav-link" data-widget="fullscreen" href="#" role="button">
                        <i class="fas fa-expand-arrows-alt"></i>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" data-widget="control-sidebar" data-slide="true" href="#" role="button">
                        <i class="fas fa-th-large"></i>
                    </a>
                </li>
            </ul>
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="/" class="brand-link">
                <img src="/dashboard/img/AdminLTELogo.png" alt="AdminLTE Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
                <span class="brand-text font-weight-light">@Configuration["App:Shortname"]</span>
            </a>

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="/dashboard/img/avatar6.jpg" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                        <a href="/user/profile" class="d-block">@User.FullName()</a>
                    </div>
                </div>

                <!-- SidebarSearch Form -->
                <div class="form-inline">
                    <div class="input-group" data-widget="sidebar-search">
                        <input class="form-control form-control-sidebar" type="search" placeholder="Search" aria-label="Search">
                        <div class="input-group-append">
                            <button class="btn btn-sidebar">
                                <i class="fas fa-search fa-fw"></i>
                            </button>
                        </div>
                    </div>
                </div>

                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <!-- Add icons to the links using the .nav-icon class with font-awesome or any other icon font library -->
                        <li class="nav-item">
                            <a href="/dashboard" class="nav-link active">
                                <i class="nav-icon fa-solid fa-chart-line"></i>
                                <p>
                                    Dashboard
                                </p>
                            </a>
                        </li>
                        <vc:toplevel-menu></vc:toplevel-menu>
                        @if (User.HasSufficientAuthority("Admin")) {
                            <li class="nav-header">SYSTEM</li>
                            <li class="nav-item">
                                <a href="#" class="nav-link">
                                    <i class="nav-icon fa-solid fa-user-tie"></i>
                                    <p>
                                        Administration
                                        <i class="fas fa-angle-left right"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="#" class="nav-link">
                                            <i class="nav-icon fas fa-shield"></i>
                                            <p>
                                                Security
                                                <i class="right fas fa-angle-left"></i>
                                            </p>
                                        </a>
                                        <ul class="nav nav-treeview">
                                            <li class="nav-item">
                                                <a href="/Certificate/New" class="nav-link">
                                                    <i class="far fa-circle nav-icon"></i>
                                                    <p>New SSL Certificate</p>
                                                </a>
                                            </li>
                                        </ul>
                                    </li>
                                    <li class="nav-item">
                                        <a href="#" class="nav-link">
                                            <i class="nav-icon fas fa-circle-info"></i>
                                            <p>
                                                Support
                                                <i class="right fas fa-angle-left"></i>
                                            </p>
                                        </a>
                                        <ul class="nav nav-treeview">
                                            <li class="nav-item">
                                                <a href="/Assistant" class="nav-link">
                                                    <i class="far fa-circle nav-icon"></i>
                                                    <p>Assistant</p>
                                                </a>
                                            </li>
                                        </ul>
                                    </li>
                                </ul>
                            </li>
                            <li class="nav-item">
                                <a href="#" class="nav-link">
                                    <i class="nav-icon far fa-file"></i>
                                    <p>
                                        Content
                                        <i class="fas fa-angle-left right"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="/Page" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>Pages</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Section" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>Sections</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Category" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>Categories</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Article" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>Articles</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Product" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>Products</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Reference" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>References</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Schedule" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>Schedule</p>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                            <li class="nav-item">
                                <a href="#" class="nav-link">
                                    <i class="nav-icon fa-solid fa-gear"></i>
                                    <p>
                                        Settings
                                        <i class="fas fa-angle-left right"></i>
                                    </p>
                                </a>
                                <ul class="nav nav-treeview">
                                    <li class="nav-item">
                                        <a href="#" class="nav-link">
                                            <i class="nav-icon fas fa-user"></i>
                                            <p>
                                                Accounts
                                                <i class="right fas fa-angle-left"></i>
                                            </p>
                                        </a>
                                        <ul class="nav nav-treeview">
                                            <li class="nav-item">
                                                <a href="/Client" class="nav-link">
                                                    <i class="far fa-circle nav-icon"></i>
                                                    <p>Clients</p>
                                                </a>
                                            </li>
                                            <li class="nav-item">
                                                <a href="/User" class="nav-link">
                                                    <i class="fa-regular fa-circle nav-icon"></i>
                                                    <p>Users</p>
                                                </a>
                                            </li>
                                            <li class="nav-item">
                                                <a href="/Role" class="nav-link">
                                                    <i class="fa-regular fa-circle nav-icon"></i>
                                                    <p>Role</p>
                                                </a>
                                            </li>
                                        </ul>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Swagger" class="nav-link" target="_blank">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>API Documentation</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Config" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>Configuration</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Menu" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>Menus</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Features" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>Features</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Activity" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>Activity</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Secret" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>Secrets</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/Status" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>Status</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/dashboard/initialize" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>Initialize Database</p>
                                        </a>
                                    </li>
                                    <li class="nav-item">
                                        <a href="/dashboard/seed" class="nav-link">
                                            <i class="fa-regular fa-circle nav-icon"></i>
                                            <p>Seed Database</p>
                                        </a>
                                    </li>
                                </ul>
                            </li>
                        }
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        @RenderBody()

        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
        <!-- Main Footer -->
        <footer class="main-footer">
            <strong>Copyright &copy; 2014-2021 <a href="https://adminlte.io">AdminLTE.io</a>.</strong>
            All rights reserved.
            <div class="float-right d-none d-sm-inline-block">
                <b>Version</b> 3.2.0
            </div>
        </footer>

    </div>
    <!-- ./wrapper -->
    <!-- REQUIRED SCRIPTS -->
    <!-- jQuery -->
    <script src="/common/plugins/jquery/jquery.min.js"></script>

    <!-- Bootstrap -->
    <script src="/common/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- AdminLTE -->
    <script src="/dashboard/js/adminlte.js"></script>

    <!-- OPTIONAL SCRIPTS -->
    <script src="/common/plugins/chart.js/Chart.min.js"></script>

    <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
    <script src="/dashboard/js/pages/dashboard3.js"></script>

    <!-- Utilities -->
    <script src="~/common/js/utils.js"></script>

    <!-- EBay Affiliate -->
    <script>window._epn = { campaign: 5339066993 };</script>
    <script src="https://epnt.ebay.com/static/epn-smart-tools.js"></script>

    <!-- Page Specific Scripts -->
    @RenderSection("Scripts", required: false)

    <!-- Syncfusion ASP.NET Core Script Manager -->
    <ejs-scripts></ejs-scripts>
    <bold-script-manager></bold-script-manager>

</body>
</html>
